@using Presentation.Extensions
@using EntitiesServices.Work_Classes;
@model SystemBRPresentation.ViewModels.ContratoViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="page-title"> 
        <h3 class="title">Contratos</h3> 
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarAdmin", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaContrato", "GestaoComercial")">Contratos</a>
            </li>
            <li class="active">
                <strong>Contrato - Processo</strong>
            </li>
        </ol>
        <br />
    </div>

    <div class="row">
        <div class="col-lg-2">
            <div class="widget style1 yellow-bg">
                <div class="row vertical-align">
                    <div class="col-xs-3">
                        <i class="fa fa-thumbs-o-up fa-3x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        @if (Model.CONT_IN_CANCELADO == 1)
	                    {
                            <h3 class="font-normal">Cancelado</h3>
                        }
                        else
                        {
                            <h3 class="font-normal">Ativo</h3>
                        }
                    </div>
                </div>
            </div>
        </div>
        @if (Model.CONT_IN_APROVADO == 1)
        {
            <div class="col-lg-2">
                <div class="widget style1 blue-bg">
                    <div class="row vertical-align">
                        <div class="col-xs-3">
                            <i class="fa fa-thumbs-o-up fa-3x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            <h3 class="font-normal">Aprovado</h3>
                        </div>
                    </div>
                </div>
            </div>
        }

        @if (Model.CONT_IN_CANCELADO == 0)
        {
            <div class="col-lg-2">
                <div class="widget style1 navy-bg">
                    <div class="row vertical-align">
                        <div class="col-xs-3">
                            <i class="fa fa-user fa-3x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            @if (Model.CONT_IN_WORKFLOW == 1)
	                        {
                                <h3 class="font-normal">Aprovação</h3>
                            }
                            else if (Model.CONT_IN_WORKFLOW == 2)
                            {
                                <h3 class="font-normal">Preparação</h3>
                            }
                            else if (Model.CONT_IN_WORKFLOW == 3)
                            {
                                <h3 class="font-normal">Operação</h3>
                            }
                            else if (Model.CONT_IN_WORKFLOW == 4)
                            {
                                <h3 class="font-normal">Encerramento</h3>
                            }

                        </div>
                    </div>
                </div>
            </div>
        }
        @if (Model.CONT_IN_CANCELADO == 0 & Model.CONT_IN_WORKFLOW == 3)
        {
            <div class="col-lg-3">
                <div class="widget style1 red-bg">
                    <div class="row vertical-align">
                        <div class="col-xs-3">
                            <i class="fa fa-user fa-3x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            <h3 class="font-normal">Em Atraso: @SessionMocks.contratoAtraso</h3>
                        </div>
                    </div>
                </div>
            </div>
        }
        @if (Model.CONT_IN_CANCELADO == 0 & Model.CONT_IN_WORKFLOW == 3)
        {
            <div class="col-lg-3">
                <div class="widget style1 red-bg">
                    <div class="row vertical-align">
                        <div class="col-xs-3">
                            <i class="fa fa-user fa-3x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            <h3 class="font-normal">Comentários: @ViewBag.comentarios</h3>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            @*<div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">*@
            <div class="col-lg-12 col-md-12 col-xs-12 ibox">
                <div class="ibox-title">
                    <h5>Contrato - Identificação & Ações Básicas</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="row">
                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">                
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Categoria
                                @Html.TextBoxFor(m => m.CATEGORIA_CONTRATO.CACT_NM_NOME, new { @class = "form-control", autofocus = true, @readonly="readonly" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Tipo de Contrato
                                @Html.TextBoxFor(m => m.TIPO_CONTRATO.TICT_NM_NOME, new { @class = "form-control", autofocus = true, @readonly="readonly" })
                                @Html.ValidationMessageFor(m => m.TICT_CD_ID, "",new { @style="color:red" })
                            </div>
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                Cliente
                                @Html.TextBoxFor(m => m.CLIENTE.CLIE_NM_NOME, new { @class = "form-control", autofocus = true, @readonly="readonly" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Nome do Contrato
                                @Html.TextBoxFor(m => m.CONT_NM_NOME, new { @class = "form-control", autofocus = true, @readonly="readonly" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Número
                                @Html.TextBoxFor(m => m.CONT_NM_NUMERO_CONTRATO, new { @class = "form-control", autofocus = true, @readonly="readonly" })
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div class="row">
                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">   
                            <div class="col-lg-12 col-md-12 col-xs-12">
                                <h5>Ações Básicas no Contrato</h5>
                                @if (Model.CONT_IN_CANCELADO == 0)
	                            {
                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                        @Html.ButtonLinkIndex(text: "Editar Contrato", action: "EditarContrato", controller: "GestaoComercial", buttonClass: "success", routeValues: new { id = Model.CONT_CD_ID })
                                    </div>
                                    if (Model.CONT_IN_ATIVO == 1)
                                    {
                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                            @Html.ButtonLinkIndex(text: "Excluir Contrato", action: "ExcluirContrato", controller: "GestaoComercial", buttonClass: "warning", routeValues: new { id = Model.CONT_CD_ID })
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                            @Html.ButtonLinkIndex(text: "Reativar Contrato", action: "ReativarContrato", controller: "GestaoComercial", buttonClass: "warning", routeValues: new { id = Model.CONT_CD_ID })
                                        </div>
                                    }
                                    if ((ViewBag.Usuario as EntitiesServices.Model.USUARIO).COLABORADOR.COLA_IN_GERENTE == 1)
	                                {
                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                            @Html.ButtonLinkIndex(text: "Cancelar Contrato", action: "CancelarContrato", controller: "GestaoComercial")
                                        </div>
	                                }
	                            } 
                                <div class="col-lg-2 col-md-2 col-xs-12">
                                    @Html.ButtonLinkIndex(text: "Visualizar Contrato", action: "VisualizarContrato", controller: "GestaoComercial", buttonClass: "success", routeValues: new { id = Model.CONT_CD_ID })
                                </div>                               
                                <div class="col-lg-2 col-md-2 col-xs-12">
                                    @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarContrato", controller: "GestaoComercial")
                                </div>                                
                            </div>
                        </div>
                    </div>

                </div>
            </div>
         </div>
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Contrato - Fluxo de Processo</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="tabs-container">
                            <ul class="nav nav-tabs">
                                @if (Model.CONT_IN_WORKFLOW == 1)
	                            {
                                    <li class="active"><a data-toggle="tab" href="#tab-1">Aprovação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-2">Preparação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-3">Operação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-4">Encerramento</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-5">Resumos</a></li>
	                            }   
                                else if (Model.CONT_IN_WORKFLOW == 2)
	                            {
                                    <li class=""><a data-toggle="tab" href="#tab-1">Aprovação</a></li>
                                    <li class="active"><a data-toggle="tab" href="#tab-2">Preparação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-3">Operação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-4">Encerramento</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-5">Resumos</a></li>
	                            }   
                                else if (Model.CONT_IN_WORKFLOW == 3)
	                            {
                                    <li class=""><a data-toggle="tab" href="#tab-1">Aprovação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-2">Preparação</a></li>
                                    <li class="active"><a data-toggle="tab" href="#tab-3">Operação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-4">Encerramento</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-5">Resumos</a></li>
	                            }   
                                else if (Model.CONT_IN_WORKFLOW == 4)
	                            {
                                    <li class=""><a data-toggle="tab" href="#tab-1">Aprovação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-2">Preparação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-3">Operação</a></li>
                                    <li class="active"><a data-toggle="tab" href="#tab-4">Encerramento</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-5">Resumos</a></li>
	                            }   
                                else if (Model.CONT_IN_WORKFLOW == 5)
	                            {
                                    <li class=""><a data-toggle="tab" href="#tab-1">Aprovação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-2">Preparação</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-3">Operação</a></li>
                                    <li class="active"><a data-toggle="tab" href="#tab-4">Encerramento</a></li>
                                    <li class=""><a data-toggle="tab" href="#tab-5">Resumos</a></li>
	                            }   
                            </ul>

                            <div class="tab-content">
                                @*Aprovação*@
                                @if (Model.CONT_IN_WORKFLOW == 1)
	                            {
                                    <div id="tab-1" class="tab-pane active">
                                        <div class="panel-body">

                                            @*Solicitação de Aprovação*@
                                            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                                                <div class="ibox-title">
                                                    <h5>Solicitação de Aprovação</h5>
                                                    <div class="ibox-tools">
                                                        <a class="collapse-link">
                                                            <i class="fa fa-chevron-up"></i>
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="ibox-content">
                                                    @if (Model.CONT_IN_ENVIO_APROVACAO == 1)
	                                                {
                                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">   
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Data de Envio
                                                                @Html.TextBoxFor(m => m.CONT_DT_SOLICITACAO_APROVACAO, "{0:dd/MM/yyyy}", new { @class = "form-control", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                            <div class="col-lg-4 col-md-4 col-xs-12">
                                                                Enviar Para
                                                                @Html.TextBoxFor(m => m.CONT_NM_RESPONSAVEL, new { @class = "form-control", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Já enviada?
                                                                <br />
                                                                @Html.CheckBoxFor(m => m.EnvioAprovacao, new { @class = "checkbox i-checks", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                        </div>
                                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">                
                                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                                <br />
                                                                Texto da mensagem*
                                                                @Html.TextAreaFor(m => m.CONT_DS_APROVACAO, new { @class = "form-control", style = "width: 980px; height: 100px;", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                        </div>
	                                                }
                                                    @if (Model.CONT_IN_ENVIO_APROVACAO == 0 & Model.CONT_IN_CANCELADO == 0)
                                                    {
                                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">  
                                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                                <br />
                                                                @Html.ButtonLinkIndex(text: "Gerar Solicitação de Aprovação", action: "SolicitarAprovacaoContrato", controller: "GestaoComercial", buttonClass: "success")
                                                            </div>
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        if (Model.CONT_IN_CANCELADO == 0 & Model.CONT_DT_SOLICITACAO_APROVACAO.Value.Date.AddDays(5) < DateTime.Today.Date)
                                                        {
                                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">  
                                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                                    <br />
                                                                    @Html.ButtonLinkIndex(text: "Lembrar Solicitação de Aprovação", action: "LembrarAprovacaoContrato", controller: "GestaoComercial", htmlAttributes: new { id = "basictoast1" } )
                                                                </div>
                                                            </div>
                                                        }
                                                    }
                                                </div>
                                            </div>

                                            @*Retorno de Aprovação*@
                                            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                                                <div class="ibox-title">
                                                    <h5>Retornos da Solicitação de Aprovação</h5>
                                                    <div class="ibox-tools">
                                                        <a class="collapse-link">
                                                            <i class="fa fa-chevron-up"></i>
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="ibox-content">
                                                    @if (Model.CONT_IN_CANCELADO == 0 & Model.CONT_IN_ENVIO_APROVACAO == 1 & Model.CONT_IN_APROVADO == 0)
	                                                {
                                                        if ((ViewBag.Usuario as EntitiesServices.Model.USUARIO).COLABORADOR.COLA_IN_APROVADOR_CONTRATO == 1)
	                                                    {
                                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">   
                                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                                    @Html.ButtonLinkIndex(text: "Gerar novo retorno", action: "GerarRetornoAprovacaoContrato", controller: "GestaoComercial")
                                                                </div>
                                                            </div>
	                                                    }
	                                                }   
                                                    @if (Model.CONT_IN_CANCELADO == 0 & Model.CONT_IN_APROVADO == 1)
	                                                {
                                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">   
                                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                                Data de Aprovação
                                                                @Html.TextBoxFor(m => m.CONT_DT_APROVACAO, "{0:dd/MM/yyyy}", new { @class = "form-control", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                        </div>
	                                                }   

                                                    @if (Model.CONTRATO_SOLICITACAO_APROVACAO.Count > 0)
                                                    {
                                                        <table class="table table-hover">
                                                            <thead>
                                                                <tr>
                                                                    <th>Data</th>
                                                                    <th>Aprovado</th>
                                                                    <th>Aprovador</th>
                                                                    <th class="td-one-action">Ações</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var cond in (Model.CONTRATO_SOLICITACAO_APROVACAO.OrderByDescending(a => a.CTSA_DT_DATA)))
                                                                {
                                                                    <tr>
                                                                        <td>@cond.CTSA_DT_DATA.ToShortDateString()</td>
                                                                        @if (cond.CTSA_IN_STATUS == 1)
	                                                                    {
                                                                            <td>Sim</td>
                                                                        }
                                                                        else
	                                                                    {
                                                                            <td>Não</td>
                                                                        }
                                                                        <td>@cond.CTSA_NM_APROVADOR</td>
                                                                        <td class="td-one-action"> 
                                                                            @Html.TableLinkDetails(cond.CTSA_CD_ID, "VerRetornoAprovacaoContrato","fa-edit", "Ver Detalhes")
                                                                        </td>
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        </table>
                                                    }
                                                </div>
                                            </div>
                                       
                                            @*Mudança de Status*@
                                            <div class="row">
                                                @if (Model.CONT_IN_CANCELADO == 0 & Model.CONT_IN_APROVADO == 1 & Model.CONT_IN_WORKFLOW == 1)
	                                            {
                                                    <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">   
                                                        <div class="col-lg-12 col-md-12 col-xs-12">
                                                            @Html.ButtonLinkIndex(text: "MUDANÇA DE STATUS - PREPARAÇÃO", action: "MudarStatusContratoPreparacao", controller: "GestaoComercial", buttonClass: "success")
                                                        </div>
                                                    </div>
	                                            }   
                                           </div>
                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div id="tab-1" class="tab-pane">
                                        <div class="panel-body">

                                            @*Solicitação de Aprovação*@
                                            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                                                <div class="ibox-title">
                                                    <h5>Solicitação de Aprovação</h5>
                                                    <div class="ibox-tools">
                                                        <a class="collapse-link">
                                                            <i class="fa fa-chevron-up"></i>
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="ibox-content">
                                                    @if (Model.CONT_IN_ENVIO_APROVACAO == 1)
	                                                {
                                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">   
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Data de Envio
                                                                @Html.TextBoxFor(m => m.CONT_DT_SOLICITACAO_APROVACAO, "{0:dd/MM/yyyy}", new { @class = "form-control", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                            <div class="col-lg-4 col-md-4 col-xs-12">
                                                                Enviar Para
                                                                @Html.TextBoxFor(m => m.CONT_NM_RESPONSAVEL, new { @class = "form-control", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                                Já enviada?
                                                                <br />
                                                                @Html.CheckBoxFor(m => m.EnvioAprovacao, new { @class = "checkbox i-checks", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                        </div>
                                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">                
                                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                                <br />
                                                                Texto da mensagem*
                                                                @Html.TextAreaFor(m => m.CONT_DS_APROVACAO, new { @class = "form-control", style = "width: 980px; height: 100px;", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                        </div>
	                                                }
                                                </div>
                                            </div>

                                            @*Retorno de Aprovação*@
                                            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                                                <div class="ibox-title">
                                                    <h5>Retornos da Solicitação de Aprovação</h5>
                                                    <div class="ibox-tools">
                                                        <a class="collapse-link">
                                                            <i class="fa fa-chevron-up"></i>
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="ibox-content">
                                                    @if (Model.CONT_IN_CANCELADO == 0 & Model.CONT_IN_APROVADO == 1)
	                                                {
                                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">   
                                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                                Data de Aprovação
                                                                @Html.TextBoxFor(m => m.CONT_DT_APROVACAO, "{0:dd/MM/yyyy}", new { @class = "form-control", autofocus = true, @readonly="readonly"})
                                                            </div>
                                                        </div>
	                                                }   

                                                    @if (Model.CONTRATO_SOLICITACAO_APROVACAO.Count > 0)
                                                    {
                                                        <table class="table table-hover">
                                                            <thead>
                                                                <tr>
                                                                    <th>Data</th>
                                                                    <th>Aprovado</th>
                                                                    <th>Aprovador</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var cond in (Model.CONTRATO_SOLICITACAO_APROVACAO.OrderByDescending(a => a.CTSA_DT_DATA)))
                                                                {
                                                                    <tr>
                                                                        <td>@cond.CTSA_DT_DATA.ToShortDateString()</td>
                                                                        @if (cond.CTSA_IN_STATUS == 1)
	                                                                    {
                                                                            <td>Sim</td>
                                                                        }
                                                                        else
	                                                                    {
                                                                            <td>Não</td>
