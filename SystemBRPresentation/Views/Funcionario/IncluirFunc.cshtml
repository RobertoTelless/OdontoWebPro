@using Presentation.Extensions
@model SystemBRPresentation.ViewModels.FuncionarioViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="page-title">
        <h3 class="title">Funcionários</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaFunc", "Funcionario")">Funcionários</a>
            </li>
            <li class="active">
                <strong>Funcionários - Inclusão</strong>
            </li>
        </ol>
        <br />
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Funcionários - Inclusão</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                    <div class="tabs-container">
                        @using (Html.BeginForm("IncluirFunc", "Funcionario", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                        {

                            <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-2">Endereços</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-3">Contatos</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-4">Documentação</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-5">Dados Pessoais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-6">Anexos</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-7">Observações</a></li>
                            </ul>

                            <div class="tab-content">
                                @*Dados Gerais*@
                                <div id="tab-1" class="tab-pane active">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    @Html.HiddenFor(m => m.FUNC_IN_ATIVO)
                                                    @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                    Situação*
                                                    @Html.DropDownListFor(m => m.SITU_CD_ID, (SelectList)ViewBag.Situacao, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.SITU_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-6 col-md-6 col-xs-12">
                                                    Função*
                                                    @Html.DropDownListFor(m => m.FNCA_CD_ID, (SelectList)ViewBag.Funcao, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FNCA_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Salário
                                                    @Html.TextBoxFor(m => m.FUNC_NR_SALARIO, "{0:n2}", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_SALARIO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-6 col-md-6 col-xs-12">
                                                    Nome*
                                                    @Html.TextBoxFor(m => m.FUNC_NM_NOME, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_NOME, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;" id="PF">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    CPF*
                                                    @Html.TextBoxFor(m => m.FUNC_NR_CPF, new { @class = "form-control", @id = "cpf", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_CPF, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    RG*
                                                    @Html.TextBoxFor(m => m.FUNC_NR_RG, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_RG, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data Emissão
                                                    @Html.TextBoxFor(m => m.FUNC_DT_EMISSAO_RG, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true, @id = "dr" })
                                                    @Html.ValidationMessageFor(m => m.FUNC_DT_EMISSAO_RG, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Orgão Emissão
                                                    @Html.TextBoxFor(m => m.FUNC_NM_ORGAO_RG, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_ORGAO_RG, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    UF Emissão
                                                    @Html.DropDownListFor(m => m.FUNC_UF_FG_ID, (SelectList)ViewBag.UF, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_UF_FG_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data Nascimento
                                                    @Html.TextBoxFor(m => m.FUNC_DT_NASCIMENTO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true, @id = "dn" })
                                                    @Html.ValidationMessageFor(m => m.FUNC_DT_NASCIMENTO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Sexo*
                                                    @Html.DropDownListFor(m => m.SEXO_CD_ID, (SelectList)ViewBag.Sexo, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.SEXO_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Escolaridade*
                                                    @Html.DropDownListFor(m => m.ESCO_CD_ID, (SelectList)ViewBag.Escol, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.ESCO_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data Admissão
                                                    @Html.TextBoxFor(m => m.FUNC_DT_ADMISSAO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true, @id = "da" })
                                                    @Html.ValidationMessageFor(m => m.FUNC_DT_ADMISSAO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Endereços*@
                                <div id="tab-2" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    CEP
                                                    @Html.TextBoxFor(m => m.FUNC_NR_CEP, new { @class = "form-control", @id = "cep", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_CEP, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Endereço
                                                    @Html.TextBoxFor(m => m.FUNC_NM_ENDERECO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_ENDERECO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Bairro
                                                    @Html.TextBoxFor(m => m.FUNC_NM_BAIRRO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_BAIRRO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Cidade
                                                    @Html.TextBoxFor(m => m.FUNC_NM_CIDADE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_CIDADE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    UF
                                                    @Html.DropDownListFor(m => m.UF_CD_ID, (SelectList)ViewBag.UF, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.UF_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Contatos*@
                                <div id="tab-3" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    E-Mail*
                                                    @Html.TextBoxFor(m => m.FUNC_NM_EMAIL, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_EMAIL, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Telefone
                                                    @Html.TextBoxFor(m => m.FUNC_NR_TELEFONE, new { @class = "form-control", autofocus = true, placeholder = "(99) 9999-9999 R: 99", @id = "tel" })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_TELEFONE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Celular
                                                    @Html.TextBoxFor(m => m.FINC_NR_CELULAR, new { @class = "form-control", autofocus = true, placeholder = "(99) 9999-9999 R: 99", @id = "cel" })
                                                    @Html.ValidationMessageFor(m => m.FINC_NR_CELULAR, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Documentação*@
                                <div id="tab-4" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    NIS
                                                    @Html.TextBoxFor(m => m.FUNC_NR_NIS, new { @class = "form-control", @id = "nis", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_NIS, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    CTPS
                                                    @Html.TextBoxFor(m => m.FUNC_NR_CTPS, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_CTPS, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Série
                                                    @Html.TextBoxFor(m => m.FUNC_NR_CTPS_SERIE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_CTPS_SERIE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Emissão
                                                    @Html.TextBoxFor(m => m.FUNC_DT_CTPS_EMISSAO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true, @id = "dt" })
                                                    @Html.ValidationMessageFor(m => m.FUNC_DT_CTPS_EMISSAO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    UF
                                                    @Html.DropDownListFor(m => m.FUNC_UF_CTPS_ID, (SelectList)ViewBag.UF, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_UF_CTPS_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Título de Eleitor
                                                    @Html.TextBoxFor(m => m.FUNC_NR_TITULO_ELEITOR, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_TITULO_ELEITOR, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Zona
                                                    @Html.TextBoxFor(m => m.FUNC_NR_ZONA, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_ZONA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Seção
                                                    @Html.TextBoxFor(m => m.FUNC_NR_SECAO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_SECAO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-23 col-xs-12">
                                                    Emissão
                                                    @Html.TextBoxFor(m => m.FUNC_DT_EMISSAO_TITULO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true, @id = "de" })
                                                    @Html.ValidationMessageFor(m => m.FUNC_DT_EMISSAO_TITULO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Município
                                                    @Html.TextBoxFor(m => m.FUNC_NM_MUNICIPIO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_MUNICIPIO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Cert.Reservista
                                                    @Html.TextBoxFor(m => m.FUNC_NR_RESERVISTA, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NR_RESERVISTA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Situação
                                                    @Html.TextBoxFor(m => m.FUNC_NM_RESERVISTA_SITUACAO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_RESERVISTA_SITUACAO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Org.Militar
                                                    @Html.TextBoxFor(m => m.FUNC_NM_RESERVISTA_OM, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_RESERVISTA_OM, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Emissão
                                                    @Html.TextBoxFor(m => m.FUNC_DT_RESERVISTA_EMISSAO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true, @id = "dc" })
                                                    @Html.ValidationMessageFor(m => m.FUNC_DT_RESERVISTA_EMISSAO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Dados Pessoais*@
                                <div id="tab-5" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Profissão
                                                    @Html.TextBoxFor(m => m.FUNC_NM_PROFISSAO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_PROFISSAO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Nome do Pai
                                                    @Html.TextBoxFor(m => m.FUNC_NM_PAI, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_PAI, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Nome da Mãe
                                                    @Html.TextBoxFor(m => m.FUNC_NM_MAE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_MAE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Nome do Cônjuge
                                                    @Html.TextBoxFor(m => m.FUNC_NM_CONJUGE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_CONJUGE, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Nacionalidade
                                                    @Html.TextBoxFor(m => m.FUNC_NM_NACIONALIDADE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_NACIONALIDADE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Naturalidade
                                                    @Html.TextBoxFor(m => m.FUNC_NM_NATURALIDADE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_NM_NATURALIDADE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    UF Naturalidade
                                                    @Html.DropDownListFor(m => m.FUNC_UF_NATURALIDADE_ID, (SelectList)ViewBag.UF, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_UF_NATURALIDADE_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Imagens - Foto e Anexos*@
                                <div id="tab-6" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div style="margin-left: 15px;" class="col-lg-12 col-md-12 col-xs-12">
                                                <div class="form-group">
                                                    <h4>Foto do Funcionario</h4><input type="file" accept=".jpg, .png, .jpeg, .gif|images/*" name="file" id="imgFunc" class="btn btn-sm btn-warning" multiple />
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <hr />
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Anexos do Cliente</h4><input class="btn btn-sm btn-warning" id="inputFile" type="file" multiple />
                                                <br />
                                                <div class="form-group">
                                                    <div class="ibox-content">
                                                        <br />
                                                        <table style="width: 100%;" class="table table-striped table-bordered table-hover dataTables-example" id="myFiles">
                                                            <thead style="background-color:lightsteelblue">
                                                                <tr>
                                                                    <td>Nome</td>
                                                                    <td>Tipo</td>
                                                                    <td>Ações</td>
                                                                </tr>
                                                            </thead>
                                                            <tbody></tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Observações*@
                                <div id="tab-7" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Observações
                                                    @Html.TextAreaFor(m => m.FUNC_TX_OBSERVACOES, new { @class = "form-control", style = "width: 1040px; height: 400px;", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.FUNC_TX_OBSERVACOES, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <br />
                                    <button class="btnSubmit btn btn-sm btn-success" id="basictoast" type="button">Confirma inclusão de Funcionário</button>
                                    @Html.ButtonLinkIndex(text: "Cancelar", action: "VoltarBaseFunc", controller: "Funcionario")
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Styles {
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/themes/base/all.css" rel="stylesheet" />
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/plugins/jasnyBootstrapStyles")
    @Styles.Render("~/plugins/toastrStyles")
}
@section Scripts {
    @Scripts.Render("~/plugins/toastr")
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/bundles/inputmask")
    @Scripts.Render("~/plugins/jasnyBootstrap")
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.maskedinput/1.4.1/jquery.maskedinput.js"></script>
    <script src="~/Scripts/uploadAnexo.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {

            $('.dataTables-example').DataTable({
                pageLength: 25,
                dom: '<"html5buttons"B>lTfgitp',
                buttons: [
                    { extend: 'copy' },
                    { extend: 'csv' },
                    { extend: 'excel', title: 'Excel' },
                    { extend: 'pdf', title: 'PDF' },

                    {
                        extend: 'print',
                        customize: function (win) {
                            $(win.document.body).addClass('white-bg');
                            $(win.document.body).css('font-size', '10px');

                            $(win.document.body).find('table')
                                .addClass('compact')
                                .css('font-size', 'inherit');
                        }
                    }
                ]

            });



        });
    </script>

    <script type="text/javascript" language="javascript">
        $(function () {
            $('.date-picker').datepicker(
                {
                    dateFormat: 'dd/mm/yy',
                    dayNames: ['Domingo', 'Segunda', 'Terça', 'Quarta', 'Quinta', 'Sexta', 'Sábado'],
                    dayNamesMin: ['D', 'S', 'T', 'Q', 'Q', 'S', 'S', 'D'],
                    dayNamesShort: ['Dom', 'Seg', 'Ter', 'Qua', 'Qui', 'Sex', 'Sáb', 'Dom'],
                    monthNames: ['Janeiro', 'Fevereiro', 'Março', 'Abril', 'Maio', 'Junho', 'Julho', 'Agosto', 'Setembro', 'Outubro', 'Novembro', 'Dezembro'],
                    monthNamesShort: ['Jan', 'Fev', 'Mar', 'Abr', 'Mai', 'Jun', 'Jul', 'Ago', 'Set', 'Out', 'Nov', 'Dez'],
                    nextText: 'Proximo',
                    prevText: 'Anterior',
                    showOn: "focus"
                }
            )
                .css("display", "normal")
                .next("button").button({
                    icons: { primary: "ui-icon-calendar" },
                    label: "Selecione uma data",
                    text: false
                });
        });
    </script>


    <script type="text/javascript">
        $(function () {
            $("#cpf").mask("999.999.999-99");
            $("#cnpj").mask("99.999.999/9999-99");
            $("#cep").mask("99999-999");
            $("#dn").mask("99/99/9999");
            $("#tel").mask("(99)9999-9999");
            $("#cel").mask("(99)99999-9999");
            $("#da").mask("99/99/9999");
            $("#nis").mask("99 999999999 99");
            $("#dc").mask("99/99/9999");
            $("#dr").mask("99/99/9999");
            $("#dt").mask("99/99/9999");
            $("#de").mask("99/99/9999");
        });
    </script>

    <script type="text/javascript">
        $(document).ready(function () {

            $('body').addClass('light-navbar');

            $("#sparkline1").sparkline([34, 43, 43, 35, 44, 32, 44, 52], {
                type: 'line',
                width: '100%',
                height: '60',
                lineColor: '#1ab394',
                fillColor: "#ffffff"
            });

        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            //Chama o evento após selecionar um valor
            $('#pessoa').on('change', function () {
                //Verifica se o valor é igual a 1 e mostra a divCnpj
                if (this.value == '2') {
                    $("#PF").hide();
                    $("#PJ").show();
                }
                //Se o tempo for mé igual a 2 mostra a divCpf
                else if (this.value == '1') {
                    $("#PJ").hide();
                    $("#PF").show();
                }
                else if (this.value == '3') {
                    $("#PF").hide();
                    $("#PJ").show();
                }

                //Se não for nem 1 nem 2 esconde as duas
                else {
                    $("#PJ").hide();
                    $("#PF").hide();
                }
            });
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {

            var i = -1;
            var toastCount = 0;
            var $toastlast;
            var getMessage = function () {
                var msg = 'Hi, welcome to Inspinia. This is example of Toastr notification box.';
                return msg;
            };

            $('#basictoast').click(function () {
                // Display a success toast, with a title
                toastr.success('Inclusão em andamento!')
            });
            $('#showtoast').click(function () {
                var shortCutFunction = $("#toastTypeGroup input:radio:checked").val();
                var msg = $('#message').val();
                var title = $('#title').val() || '';
                var $showDuration = $('#showDuration');
                var $hideDuration = $('#hideDuration');
                var $timeOut = $('#timeOut');
                var $extendedTimeOut = $('#extendedTimeOut');
                var $showEasing = $('#showEasing');
                var $hideEasing = $('#hideEasing');
                var $showMethod = $('#showMethod');
                var $hideMethod = $('#hideMethod');
                var toastIndex = toastCount++;
                toastr.options = {
                    closeButton: $('#closeButton').prop('checked'),
                    debug: $('#debugInfo').prop('checked'),
                    progressBar: $('#progressBar').prop('checked'),
                    positionClass: $('#positionGroup input:radio:checked').val() || 'toast-top-right',
                    onclick: null
                };
                if ($('#addBehaviorOnToastClick').prop('checked')) {
                    toastr.options.onclick = function () {
                        alert('You can perform some custom action after a toast goes away');
                    };
                }
                if ($showDuration.val().length) {
                    toastr.options.showDuration = $showDuration.val();
                }
                if ($hideDuration.val().length) {
                    toastr.options.hideDuration = $hideDuration.val();
                }
                if ($timeOut.val().length) {
                    toastr.options.timeOut = $timeOut.val();
                }
                if ($extendedTimeOut.val().length) {
                    toastr.options.extendedTimeOut = $extendedTimeOut.val();
                }
                if ($showEasing.val().length) {
                    toastr.options.showEasing = $showEasing.val();
                }
                if ($hideEasing.val().length) {
                    toastr.options.hideEasing = $hideEasing.val();
                }
                if ($showMethod.val().length) {
                    toastr.options.showMethod = $showMethod.val();
                }
                if ($hideMethod.val().length) {
                    toastr.options.hideMethod = $hideMethod.val();
                }
                if (!msg) {
                    msg = getMessage();
                }
                $("#toastrOptions").text("Command: toastr["
                    + shortCutFunction
                    + "](\""
                    + msg
                    + (title ? "\", \"" + title : '')
                    + "\")\n\ntoastr.options = "
                    + JSON.stringify(toastr.options, null, 2)
                );
                var $toast = toastr[shortCutFunction](msg, title); // Wire up an event handler to a button in the toast, if it exists
                $toastlast = $toast;
                if ($toast.find('#okBtn').length) {
                    $toast.delegate('#okBtn', 'click', function () {
                        alert('you clicked me. i was toast #' + toastIndex + '. goodbye!');
                        $toast.remove();
                    });
                }
                if ($toast.find('#surpriseBtn').length) {
                    $toast.delegate('#surpriseBtn', 'click', function () {
                        alert('Surprise! you clicked me. i was toast #' + toastIndex + '. You could perform an action here.');
                    });
                }
            });
            function getLastToast() {
                return $toastlast;
            }
            $('#clearlasttoast').click(function () {
                toastr.clear(getLastToast());
            });
            $('#cleartoasts').click(function () {
                toastr.clear();
            });

        });
    </script>

    <script>
        SendFiles('imgFunc', 'Funcionario', 'IncluirFunc', 'UploadFileFunc_Inclusao', 'MontarTelaFunc');
    </script>

    @if (ViewBag.Message != null)
    {
        <script type="text/javascript">
            window.onload = function () {
                  alert("@ViewBag.Message");
            };
        </script>
    }
}
