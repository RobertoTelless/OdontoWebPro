@using Presentation.Extensions
@model SystemBRPresentation.ViewModels.ProdutoViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="page-title">
        <h3 class="title">Produtos</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaProduto", "Produto")">Produtos</a>
            </li>
            <li class="active">
                <strong>Produto - Inclusão</strong>
            </li>
        </ol>
        <br />
    </div>

    @*TABS - PRINCIPAL*@
    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Produto - Inclusão</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                    <div class="tabs-container">
                        @using (Html.BeginForm("IncluirProduto", "Produto", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                        {
                            <div class="form-group">
                                <button class="btnSubmit btn btn-sm btn-success" id="basictoast" type="button">Confirma inclusão de Produto</button>
                                @Html.ButtonLinkIndex(text: "Cancelar", action: "VoltarBaseProduto", controller: "Produto")
                            </div>
                            <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-2">Estoque</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-3">Varejo</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-4">Food Service</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-5">Informações Tributárias</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-6">Expedição</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-7">E-Commerce</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-8">Preços e Custos</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-9">Imagens</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-10">Observações</a></li>
                            </ul>
                            <div class="tab-content">

                                @*Dados Gerais*@
                                <div id="tab-1" class="tab-pane active">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    @Html.HiddenFor(m => m.PROD_CD_ID)
                                                    @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                    @Html.HiddenFor(m => m.PROD_DT_CADASTRO)
                                                    @Html.HiddenFor(m => m.PROD_IN_ATIVO)
                                                    @Html.HiddenFor(m => m.MATR_CD_ID)
                                                    @Html.HiddenFor(m => m.FILI_CD_ID)
                                                    @Html.HiddenFor(m => m.PROD_AQ_FOTO)
                                                    Categoria*
                                                    @Html.DropDownListFor(m => m.CAPR_CD_ID, (SelectList)ViewBag.Tipos, "Selecionar", new { @class = "form-control", autofocus = true, onchange = "FiltrarSubCategoriaProduto($(this).val())" })
                                                    @Html.ValidationMessageFor(m => m.CAPR_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Subcategoria
                                                    @Html.DropDownListFor(m => m.SCPR_CD_ID, (SelectList)ViewBag.Subs, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.SCPR_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Tipo de Produto*
                                                    @Html.DropDownListFor(m => m.PROD_IN_TIPO_PRODUTO, (SelectList)ViewBag.TiposProduto, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_IN_TIPO_PRODUTO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Produto Composto?
                                                    <br />
                                                    @Html.CheckBoxFor(m => m.Composto, new { @class = "checkbox i-checks" })
                                                    @Html.ValidationMessageFor(m => m.Composto, "", new { @style = "color:red" })
                                                </div>
                                            </div>

                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Código de Barras (Número)
                                                    @Html.TextBoxFor(m => m.PROD_NR_BARCODE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_BARCODE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Unidade
                                                    @Html.DropDownListFor(m => m.UNID_CD_ID, (SelectList)ViewBag.Unidades, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.UNID_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-6 col-md-6 col-xs-12">
                                                    Nome*
                                                    @Html.TextBoxFor(m => m.PROD_NM_NOME, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NM_NOME, "", new { @style = "color:red" })
                                                </div>
                                            </div>

                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Descrição
                                                    @Html.TextAreaFor(m => m.PROD_DS_DESCRICAO, new { @class = "form-control", style = "width: 100%; height: 100px;", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_DS_DESCRICAO, "", new { @style = "color:red" })
                                                </div>
                                            </div>

                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Informações do Produto
                                                    @Html.TextAreaFor(m => m.PROD_DS_INFORMACOES, new { @class = "form-control", style = "width: 100%; height: 200px;", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_DS_INFORMACOES, "", new { @style = "color:red" })
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>

                                @*Estoque*@
                                <div id="tab-2" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    @Html.HiddenFor(m => m.PROD_QN_QUANTIDADE_INICIAL)
                                                    Estoque Máximo
                                                    @Html.TextBoxFor(m => m.PROD_QN_QUANTIDADE_MAXIMA, new { @class = "form-control touchspin2", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_QN_QUANTIDADE_MAXIMA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Estoque Mínimo
                                                    @Html.TextBoxFor(m => m.PROD_QN_QUANTIDADE_MINIMA, new { @class = "form-control touchspin2", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_QN_QUANTIDADE_MINIMA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Reserva de Estoque
                                                    @Html.TextBoxFor(m => m.PROD_QN_RESERVA_ESTOQUE, new { @class = "form-control touchspin2", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_QN_RESERVA_ESTOQUE, "", new { @style = "color:red" })
                                                </div>
                                                @*<div class="col-lg-2 col-md-2 col-xs-12">
                                                    Estoque Inicial
                                                    @Html.TextBoxFor(m => m.PROD_QN_QUANTIDADE_INICIAL, new { @class = "form-control touchspin2", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_QN_QUANTIDADE_INICIAL, "", new { @style = "color:red" })
                                                </div>*@
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Aviso de Limite Mínimo
                                                    <br />
                                                    @Html.CheckBoxFor(m => m.AvisaMinima, new { @class = "checkbox i-checks", @id = "AvisaMinima" })
                                                    @Html.ValidationMessageFor(m => m.AvisaMinima, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Varejo*@
                                <div id="tab-3" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Marca
                                                    @Html.TextBoxFor(m => m.PROD_NM_MARCA, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NM_MARCA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Modelo
                                                    @Html.TextBoxFor(m => m.PROD_NM_MODELO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NM_MODELO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Fabricante
                                                    @Html.TextBoxFor(m => m.PROD_NM_FABRICANTE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NM_FABRICANTE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Referencia Fabricante
                                                    @Html.TextBoxFor(m => m.PROD_NM_REFERENCIA_FABRICANTE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NM_REFERENCIA_FABRICANTE, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Referência
                                                    @Html.TextBoxFor(m => m.PROD_NR_REFERENCIA, new { @class = "form-control", @style = "color:green", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_REFERENCIA, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Garantia (dias)
                                                    @Html.TextBoxFor(m => m.Garantia, new { @class = "form-control touchspin2", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.Garantia, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Food Service*@
                                <div id="tab-4" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Balança PDV
                                                    <br />
                                                    @Html.CheckBoxFor(m => m.BalancaPDV, new { @class = "checkbox i-checks", @id = "BalancaPDV" })
                                                    @Html.ValidationMessageFor(m => m.BalancaPDV, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Balança Retaguarda
                                                    <br />
                                                    @Html.CheckBoxFor(m => m.BalancaRetaguarda, new { @class = "checkbox i-checks", @id = "BalancaRetaguarda" })
                                                    @Html.ValidationMessageFor(m => m.BalancaRetaguarda, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Validade
                                                    @Html.TextBoxFor(m => m.PROD_NR_VALIDADE, new { @class = "form-control touchspin2", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_VALIDADE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Produto Tipo Combo
                                                    <br />
                                                    @Html.CheckBoxFor(m => m.ProdutoTipoCombo, new { @class = "checkbox i-checks", @id = "ProdutoTipoCombo" })
                                                    @Html.ValidationMessageFor(m => m.ProdutoTipoCombo, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Produto Opção Combo
                                                    <br />
                                                    @Html.CheckBoxFor(m => m.ProdutoOpcaoCombo, new { @class = "checkbox i-checks", @id = "ProdutoOpcaoCombo" })
                                                    @Html.ValidationMessageFor(m => m.ProdutoOpcaoCombo, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Divisão
                                                    @Html.TextBoxFor(m => m.PROD_IN_DIVISAO, new { @class = "form-control touchspin1", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_IN_DIVISAO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Cobrar pelo Maior
                                                    <br />
                                                    @Html.CheckBoxFor(m => m.CobrarMaior, new { @class = "checkbox i-checks", @id = "CobrarMaior" })
                                                    @Html.ValidationMessageFor(m => m.CobrarMaior, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Gerar Arquivo Texto
                                                    <br />
                                                    @Html.CheckBoxFor(m => m.ArquivoTexto, new { @class = "checkbox i-checks", @id = "ArquivoTexto" })
                                                    @Html.ValidationMessageFor(m => m.ArquivoTexto, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Informações Nutricionais
                                                    @Html.TextAreaFor(m => m.PROD_DS_INFORMACAO_NUTRICIONAL, new { @class = "form-control", style = "width: 1040px; height: 200px;", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_DS_INFORMACAO_NUTRICIONAL, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Informações Tributarias*@
                                <div id="tab-5" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Origem
                                                    @Html.DropDownListFor(m => m.PROR_CD_ID, (SelectList)ViewBag.Origens, "Selecionar", new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROR_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    NCM
                                                    @Html.TextBoxFor(m => m.PROD_NR_NCM, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_NCM, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    GTIN/EAN
                                                    @Html.TextBoxFor(m => m.PROD_CD_GTIN_EAN, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_CD_GTIN_EAN, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    GTIN/EAN Trib.
                                                    @Html.TextBoxFor(m => m.PROD_NR_GTIN_EAN_TRIB, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_GTIN_EAN_TRIB, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    CEST
                                                    @Html.TextBoxFor(m => m.PROD_NR_CEST, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_CEST, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Unidade Tributável
                                                    @Html.TextBoxFor(m => m.PROD_NM_UNIDADE_TRIB, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NM_UNIDADE_TRIB, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Fator de Conversão
                                                    @Html.TextBoxFor(m => m.PROD_NR_FATOR_CONVERSAO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_FATOR_CONVERSAO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Cod.Enquad. IPI
                                                    @Html.TextBoxFor(m => m.PROD_NR_ENQUADRE_IPI, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_ENQUADRE_IPI, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Valor IPI Fixo (R$)
                                                    @Html.TextBoxFor(m => m.PROD_VL_IPI_FIXO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_VL_IPI_FIXO, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*Expedição*@
                                <div id="tab-6" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Código GTIN/EAN
                                                    @Html.TextBoxFor(m => m.PROD_CD_GTIN_EAN, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_CD_GTIN_EAN, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Localização do Estoque
                                                    @Html.TextBoxFor(m => m.PROD_NM_LOCALIZACAO_ESTOQUE, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NM_LOCALIZACAO_ESTOQUE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Peso Líquido (Kg)
                                                    @Html.TextBoxFor(m => m.PROD_QN_PESO_LIQUIDO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_QN_PESO_LIQUIDO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Peso Bruto (Kg)
                                                    @Html.TextBoxFor(m => m.PROD_QN_PESO_BRUTO, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_QN_PESO_BRUTO, "", new { @style = "color:red" })
                                                </div>
                                            </div>

                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Tipo de Embalagem
                                                    @Html.DropDownListFor(m => m.PROD_IN_TIPO_EMBALAGEM, (SelectList)ViewBag.TiposEmbalagem, "Selecionar", new { @class = "form-control" })
                                                    @Html.ValidationMessageFor(m => m.PROD_IN_TIPO_EMBALAGEM, "", new { @style = "color:red" })
                                                </div>
                                                <div id="altura" class="col-lg-2 col-md-2 col-xs-12">
                                                    Altura (cm)
                                                    @Html.TextBoxFor(m => m.PROD_NR_ALTURA, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_ALTURA, "", new { @style = "color:red" })
                                                </div>
                                                <div id="largura" class="col-lg-2 col-md-2 col-xs-12">
                                                    Largura (cm)
                                                    @Html.TextBoxFor(m => m.PROD_NR_LARGURA, new { @class = "form-control", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.PROD_NR_LARGURA, "", new { @style = "color:red" })
                                                </div>
