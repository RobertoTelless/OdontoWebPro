    <script type="text/javascript">
        $(document).ready(function () {

            var i = -1;
            var toastCount = 0;
            var $toastlast;
            var getMessage = function () {
                var msg = 'Hi, welcome to Inspinia. This is example of Toastr notification box.';
                return msg;
            };

            $('#basictoast').click(function () {
                // Display a success toast, with a title
                toastr.success('Inclusão em andamento!')
            });
            $('#showtoast').click(function () {
                var shortCutFunction = $("#toastTypeGroup input:radio:checked").val();
                var msg = $('#message').val();
                var title = $('#title').val() || '';
                var $showDuration = $('#showDuration');
                var $hideDuration = $('#hideDuration');
                var $timeOut = $('#timeOut');
                var $extendedTimeOut = $('#extendedTimeOut');
                var $showEasing = $('#showEasing');
                var $hideEasing = $('#hideEasing');
                var $showMethod = $('#showMethod');
                var $hideMethod = $('#hideMethod');
                var toastIndex = toastCount++;
                toastr.options = {
                    closeButton: $('#closeButton').prop('checked'),
                    debug: $('#debugInfo').prop('checked'),
                    progressBar: $('#progressBar').prop('checked'),
                    positionClass: $('#positionGroup input:radio:checked').val() || 'toast-top-right',
                    onclick: null
                };
                if ($('#addBehaviorOnToastClick').prop('checked')) {
                    toastr.options.onclick = function () {
                        alert('You can perform some custom action after a toast goes away');
                    };
                }
                if ($showDuration.val().length) {
                    toastr.options.showDuration = $showDuration.val();
                }
                if ($hideDuration.val().length) {
                    toastr.options.hideDuration = $hideDuration.val();
                }
                if ($timeOut.val().length) {
                    toastr.options.timeOut = $timeOut.val();
                }
                if ($extendedTimeOut.val().length) {
                    toastr.options.extendedTimeOut = $extendedTimeOut.val();
                }
                if ($showEasing.val().length) {
                    toastr.options.showEasing = $showEasing.val();
                }
                if ($hideEasing.val().length) {
                    toastr.options.hideEasing = $hideEasing.val();
                }
                if ($showMethod.val().length) {
                    toastr.options.showMethod = $showMethod.val();
                }
                if ($hideMethod.val().length) {
                    toastr.options.hideMethod = $hideMethod.val();
                }
                if (!msg) {
                    msg = getMessage();
                }
                $("#toastrOptions").text("Command: toastr["
                    + shortCutFunction
                    + "](\""
                    + msg
                    + (title ? "\", \"" + title : '')
                    + "\")\n\ntoastr.options = "
                    + JSON.stringify(toastr.options, null, 2)
                );
                var $toast = toastr[shortCutFunction](msg, title); // Wire up an event handler to a button in the toast, if it exists
                $toastlast = $toast;
                if ($toast.find('#okBtn').length) {
                    $toast.delegate('#okBtn', 'click', function () {
                        alert('you clicked me. i was toast #' + toastIndex + '. goodbye!');
                        $toast.remove();
                    });
                }
                if ($toast.find('#surpriseBtn').length) {
                    $toast.delegate('#surpriseBtn', 'click', function () {
                        alert('Surprise! you clicked me. i was toast #' + toastIndex + '. You could perform an action here.');
                    });
                }
            });
            function getLastToast() {
                return $toastlast;
            }
            $('#clearlasttoast').click(function () {
                toastr.clear(getLastToast());
            });
            $('#cleartoasts').click(function () {
                toastr.clear();
            });

        });
    </script>

    <script>
        function FiltrarSubCategoriaProduto(CAPR_CD_ID) {
            var data = {
                id: CAPR_CD_ID
            }

            var objSubCatProduto = $('select#SCPR_CD_ID');

            // Requisição para o controller
            $.ajax({
                url: '../Produto/FiltrarSubCategoriaProduto'
                , type: 'POST'
                , data: data
                , success: function (r) {
                    // Remove todos os itens da lista e adiciona o placeholder
                    objSubCatProduto.children('option').remove();
                    objSubCatProduto.append('<option>Selecionar</option>');

                    // Loop para adicionar Itens específicos à categoria
                    r.forEach(function (e) {
                        objSubCatProduto.append('<option value="' + e.SCPR_CD_ID + '">' + e.SCPR_NM_NOME + '</option>');
                    });

                    //console.log(r);
                }
            });
        }
    </script>

    <script>
        function incluiPrecoProduto(cdFilial, nmFilial, preco, precoPromo) {
            $('table#precos td.dataTables_empty').closest('tr').remove();

            $('table#precos tbody').append('<tr>'
                + '<td style="display: none">' + cdFilial + '<input type="hidden" name="rowCdFili" id="rowCdFili' + cdFilial + '" value="' + cdFilial + '"></td>'
                + '<td>' + nmFilial + '<input type="hidden" name="rowNmFili" id="rowNmFili' + cdFilial + '" value="' + nmFilial + '"></td>'
                + '<td>' + preco + '<input type="hidden" name="rowPreco" id="rowPreco' + cdFilial + '" value="' + preco + '"></td>'
                + '<td>' + precoPromo + '<input type="hidden" name="rowPrecoPromo" id="rowPrecoPromo' + cdFilial + '" value="' + precoPromo + '"></td>'
                + '<td><span style="color: #337ab7; cursor: pointer;" class="fa fa-trash-o" onclick="apagaRow($(this))"></span></td>'
            );
        }

        function apagaRow(td) {
            td.closest('tr').remove();

            if ($('table#precos tr').length == 1) {
                $('table#precos tbody').append('<tr class="odd"><td valign="top" colspan="3" class="dataTables_empty">Nenhuma linha recuperada</td></tr>');
            }
        }
    </script>

    <script>
        $(function () {
            let inputFile = $('#inputFile');
            let inputProfile = $('#imgProduto'); //$('#imgExemplo');
            let buttonSubmit = $('.btnSubmit');
            let filesContainer = $('#myFiles');
            let files = [];

            inputFile.change(function () {
                let newFiles = [];
                for (let index = 0; index < inputFile[0].files.length; index++) {
                    let file = inputFile[0].files[index];
                    newFiles.push(file);
                    files.push(file);
                }

                newFiles.forEach(file => {
                    let fileElement = $(`<tr><td>${file.name}</td><td><span class="tbl-link fa-lg fa fa-file"></span></td><td><span class="tbl-link fa-lg fa fa-trash-o"></span></td></tr>`);
                    fileElement.data('fileData', file);
                    $('.dataTables_empty').parent().remove();
                    filesContainer.append(fileElement);

                    fileElement.click(function (event) {
                        let fileElement = $(event.target);
                        let indexToRemove = files.indexOf(fileElement.data('fileData'));
                        fileElement.parent().parent().remove();
                        files.splice(indexToRemove, 1);
                    });
                });
            });

            inputProfile.change(function () {
                if ($('#profile').length == 0) {
                    let newFiles = [];
                    for (let index = 0; index < inputProfile[0].files.length; index++) {
                        let file = inputProfile[0].files[index];
                        newFiles.push(file);
                        files.push(file);
                    };

                    newFiles.forEach(file => {
                        let fileElement = $(`<tr><td id="profile">${file.name}</td><td><span class="tbl-link fa-lg fa fa-user-circle"></span></td><td class="td-one-action"><span class="tbl-link fa-lg fa fa-trash-o"></span></td></tr>`);
                        fileElement.data('fileData', file);
                        $('.dataTables_empty').parent().remove()
                        filesContainer.prepend(fileElement);

                        fileElement.click(function (event) {
                            let fileElement = $(event.target);
                            let indexToRemove = files.indexOf(fileElement.data('fileData'));
                            fileElement.parent().parent().remove();
                            files.splice(indexToRemove, 1);
                        });
                    });
                } else {
                    alert('Foto de perfil ja inclusa');
                }
            });

            buttonSubmit.click(function () {
                var json = new Array();

                $('table#precos > tbody > tr').each(function (i, e) {
                    json.push({
                        "fili_cd_id": $(e).find('input[name="rowCdFili"]').val()
                        , "fili_nm_nome": $(e).find('input[name="rowNmFili"]').val()
                        , "preco": $(e).find('input[name="rowPreco"]').val()
                        , "precoPromo": $(e).find('input[name="rowPrecoPromo"]').val()
                    });
                });

                let formData = new FormData();

                files.forEach(file => {
                    formData.append('files', file);
                });

                var model = {
                    "ASSI_CD_ID": $('#ASSI_CD_ID').val()
                    , "PROD_DT_CADASTRO": $('#PROD_DT_CADASTRO').val()
                    , "PROD_IN_ATIVO": $('#PROD_IN_ATIVO').val()
                    , "MATR_CD_ID": $('#MATR_CD_ID').val()
                    , "FILI_CD_ID": $('#FILI_CD_ID').val()
                    , "PROD_AQ_FOTO": $('#PROD_AQ_FOTO').val()
                    , "CAPR_CD_ID": $('#CAPR_CD_ID').val()
                    , "SCPR_CD_ID": $('#SCPR_CD_ID').val()
                    , "PROD_IN_TIPO_PRODUTO": $('#PROD_IN_TIPO_PRODUTO').val()
                    , "Composto": $('#Composto').iCheck('update')[0].checked
                    , "PROD_NR_BARCODE": $('#PROD_NR_BARCODE').val()
                    , "PROD_NM_NOME": $('#PROD_NM_NOME').val()
                    , "UNID_CD_ID": $('#UNID_CD_ID').val()
                    , "PrecoVenda": $('#PrecoVenda').val()
                    , "PrecoPromocao": $('#PrecoPromocao').val()
                    , "PROD_DS_DESCRICAO": $('#PROD_DS_DESCRICAO').val()
                    , "PROD_DS_INFORMACOES": $('#PROD_DS_INFORMACOES').val()
                    , "PROD_QN_QUANTIDADE_INICIAL": $('#PROD_QN_QUANTIDADE_INICIAL').val()
                    , "PROD_QN_QUANTIDADE_MAXIMA": $('#PROD_QN_QUANTIDADE_MAXIMA').val()
                    , "PROD_QN_QUANTIDADE_MINIMA": $('#PROD_QN_QUANTIDADE_MINIMA').val()
                    , "PROD_QN_RESERVA_ESTOQUE": $('#PROD_QN_RESERVA_ESTOQUE').val()
                    , "AvisaMinima": $('#AvisaMinima').iCheck('update')[0].checked
                    , "PROD_NM_MARCA": $('#PROD_NM_MARCA').val()
                    , "PROD_NM_MODELO": $('#PROD_NM_MODELO').val()
                    , "PROD_NM_FABRICANTE": $('#PROD_NM_FABRICANTE').val()
                    , "PROD_NM_REFERENCIA_FABRICANTE": $('#PROD_NM_REFERENCIA_FABRICANTE').val()
                    , "PROD_NR_REFERENCIA": $('#PROD_NR_REFERENCIA').val()
                    , "Garantia": $('#Garantia').val()
                    , "BalancaPDV": $('#BalancaPDV').iCheck('update')[0].checked
                    , "BalancaRetaguarda": $('#BalancaRetaguarda').iCheck('update')[0].checked
                    , "PROD_NR_VALIDADE": $('#PROD_NR_VALIDADE').val()
                    , "ProdutoTipoCombo": $('#ProdutoTipoCombo').iCheck('update')[0].checked
                    , "ProdutoOpcaoCombo": $('#ProdutoOpcaoCombo').iCheck('update')[0].checked
                    , "PROD_IN_DIVISAO": $('#PROD_IN_DIVISAO').val()
                    , "CobrarMaior": $('#CobrarMaior').iCheck('update')[0].checked
                    , "ArquivoTexto": $('#ArquivoTexto').iCheck('update')[0].checked
                    , "PROD_DS_INFORMACAO_NUTRICIONAL": $('#PROD_DS_INFORMACAO_NUTRICIONAL').val()
                    , "PROR_CD_ID": $('#PROR_CD_ID').val()
                    , "PROD_NR_NCM": $('#PROD_NR_NCM').val()
                    , "PROD_CD_GTIN_EAN": $('#PROD_CD_GTIN_EAN').val()
                    , "PROD_NR_GTIN_EAN_TRIB": $('#PROD_NR_GTIN_EAN_TRIB').val()
                    , "PROD_NR_CEST": $('#PROD_NR_CEST').val()
                    , "PROD_NM_UNIDADE_TRIB": $('#PROD_NM_UNIDADE_TRIB').val()
                    , "PROD_NR_FATOR_CONVERSAO": $('#PROD_NR_FATOR_CONVERSAO').val()
                    , "PROD_NR_ENQUADRE_IPI": $('#PROD_NR_ENQUADRE_IPI').val()
                    , "PROD_VL_IPI_FIXO": $('#PROD_VL_IPI_FIXO').val()
                    , "PROD_CD_GTIN_EAN": $('#PROD_CD_GTIN_EAN').val()
                    , "PROD_NM_LOCALIZACAO_ESTOQUE": $('#PROD_NM_LOCALIZACAO_ESTOQUE').val()
                    , "PROD_QN_PESO_LIQUIDO": $('#PROD_QN_PESO_LIQUIDO').val()
                    , "PROD_QN_PESO_BRUTO": $('#PROD_QN_PESO_BRUTO').val()
                    , "PROD_IN_TIPO_EMBALAGEM": $('#PROD_IN_TIPO_EMBALAGEM').val()
                    , "PROD_NR_ALTURA": $('#PROD_NR_ALTURA').val()
                    , "PROD_NR_LARGURA": $('#PROD_NR_LARGURA').val()
                    , "PROD_NR_COMPRIMENTO": $('#PROD_NR_COMPRIMENTO').val()
                    , "PROD_NR_DIAMETRO": $('#PROD_NR_DIAMETRO').val()
                    , "PROD_NM_SLUG": $('#PROD_NM_SLUG').val()
                    , "PROD_NM_KEYWORDS": $('#PROD_NM_KEYWORDS').val()
                    , "PROD_NM_TITULO_SEO": $('#PROD_NM_TITULO_SEO').val()
                    , "PROD_NR_TAG": $('#PROD_NR_TAG').val()
                    , "PROD_NM_UNIDADE_TRIB": $('#PROD_NM_UNIDADE_TRIB').val()
                    , "PROD_DS_DESCRICAO_CEO": $('#PROD_DS_DESCRICAO_CEO').val()
                    , "PROD_TX_OBSERVACOES": $('#PROD_TX_OBSERVACOES').val()

                }

                var data = {
                    "vm": model
                    , "tabelaProduto": JSON.stringify(json)
                }

                console.log('Sending...');

                $.ajax({
                    url: '../Produto/IncluirProduto'
                    , type: 'POST'
                    , datatype: 'json'
                    , data: data
                    , success: function (e) {
                        console.log(e);
                        if (e == 1) {
                            if ($('#profile').length == 0) {
                                formData.append('perfil', 0);
                            } else {
                                formData.append('perfil', 1);
                            }

                            if (files.length > 0) {
                                $.ajax({
                                    url: '../Produto/UploadFileProduto_Inclusao' //'../Exemplo/UploadFileExemplo_Inclusao'
                                    , data: formData
                                    , type: 'POST'
                                    , success: function (data) { window.open('../Produto/MontarTelaProduto', '_self'); } //'../Exemplo/MontarTelaExemplo'
                                    , error: function (data) { console.log('ERROR!!'); }
                                    , cache: false
                                    , processData: false
                                    , contentType: false
                                });
                            }
                        }
                        else {
                            if ($('.ibox-content').find('.alert').length == 0) {
                                $('.ibox-content').prepend($(e).find('.alert'));
                            } else {
                                $('.ibox-content').find('.alert').html($(e).find('.alert').html())
                            }
                        }
                    }
                });
            });
        });
    </script>

    @if (ViewBag.Message != null)
    {
        <script type="text/javascript">
            window.onload = function () {
                  alert("@ViewBag.Message");
            };
        </script>
    }
}
