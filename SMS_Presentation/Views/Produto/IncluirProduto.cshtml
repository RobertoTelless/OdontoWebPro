        function apagaRow(td) {
            td.closest('tr').remove();

            if ($('table#precos tr').length == 1) {
                $('table#precos tbody').append('<tr class="odd"><td valign="top" colspan="3" class="dataTables_empty">Nenhuma linha recuperada</td></tr>');
            }
        }
